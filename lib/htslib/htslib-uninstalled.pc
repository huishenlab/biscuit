includedir=/varidata/research/projects/shen/tools/morrison/devel/biscuit/feature_development/handle_dependencies/biscuit/lib/htslib
libdir=/varidata/research/projects/shen/tools/morrison/devel/biscuit/feature_development/handle_dependencies/biscuit/lib/htslib

# Flags and libraries needed when linking against a static libhts.a
# (used by manual and semi-manual pkg-config(1)-style enquiries).
static_ldflags=
static_libs=-lz -lm -lbz2 -llzma -lcurl

Name: htslib
Description: C library for high-throughput sequencing data formats
Version: @-PACKAGE_VERSION@
Cflags: -I${includedir}
Libs: -L${libdir} -lhts
Libs.private: -L${libdir}  -lhts -lm -lpthread
Requires.private: zlib 
